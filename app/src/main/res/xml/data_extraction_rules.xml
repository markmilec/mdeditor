<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- TODO: Add <include> and <exclude> rules here for what data should be backed up.
             By default, all files are backed up.
             Read more at https://developer.android.com/guide/topics/data/autobackup -->
        <include domain="sharedpref" path="."/>
        <exclude domain="sharedpref" path="device.xml"/>
    </cloud-backup>
    <device-transfer>
        <!-- TODO: Add <include> and <exclude> rules here for what data should be transferred.
             By default, all files are transferred.
             Read more at https://developer.android.com/guide/topics/data/autobackup# переносе-данных-между-устройствами -->
        <include domain="sharedpref" path="."/>
        <exclude domain="sharedpref" path="device.xml"/>
    </device-transfer>
</data-extraction-rules>
